{
  "name": "pmu-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/update-pwa-version.js && prisma generate && node scripts/validate-schema-sync.js && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage",
    "test:e2e": "playwright test",
    "test:all": "pnpm typecheck && pnpm lint && pnpm test && pnpm test:e2e",
    "lighthouse:ci": "node scripts/run-lhci.mjs",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:prod": "node scripts/setup-production-db.js",
    "db:studio": "prisma studio",
    "health:check": "node scripts/health-check.js",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "validate-schema": "node scripts/validate-schema-sync.js",
    "build:prod": "npm run validate-schema && npm run pre-deploy && npm run build",
    "emergency-schema-recovery": "node scripts/emergency-schema-recovery.js",
    "deploy-safe": "npm run validate-schema && npm run build && git push origin main"
  },
